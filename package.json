{
  "name": "spock-etl",
  "version": "0.0.1",
  "license": "MIT",
  "bin": "lib/core/runner.js",
  "scripts": {
    "start-etl": "ts-node -r ./loadenv.js ./packages/blockchain-etl/runner.ts",
    "start-api": "ts-node -r ./loadenv.js ./packages/api/index.ts",
    "start-validate-gbq": "ts-node -r ./loadenv.js ./packages/scripts/verify-data-integrity/google-big-query-dataset-check.ts",
    "start-rest-api": "cd packages/rest-api && yarn start",
    "build": "rm -rf ./lib && tsc && chmod +x lib/core/runner.js",
    "build:watch": "rm -rf ./lib && tsc --watch",
    "format": "prettier --list-different '**/*.{ts,json,md,gql}'",
    "format:fix": "prettier --write '**/*.{ts,json,md,gql}'",
    "tslint": "tslint -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tslint:fix": "tslint --fix --format stylish -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tsc": "tsc --noEmit",
    "lint": "yarn format && yarn tslint && yarn tsc",
    "lint:fix": "yarn format:fix && yarn tslint:fix && yarn tsc",
    "test:unit": "jest unit",
    "test:e2e": "jest --forceExit --runInBand e2e",
    "test": "yarn lint && yarn test:unit",
    "test:fix": "yarn lint:fix && yarn test:unit",
    "migrate": "ts-node -r ./loadenv.js packages/blockchain-etl/migrate.ts"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.18",
    "@types/promise-retry": "^1.1.1",
    "chai": "^4.2.0",
    "dotenv-flow": "^0.3.0",
    "jest": "^24.4.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "ts-essentials": "^2.0.8",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.2.2",
    "typestrict": "^1.0.2"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^3.0.0",
    "@types/helmet": "^0.0.43",
    "@types/node-cleanup": "^2.1.0",
    "abi-decoder": "^1.2.0",
    "bignumber.js": "^8.0.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "consola": "^2.7.1",
    "ejs": "^2.6.1",
    "ethers": "^4.0.25",
    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "lodash": "^4.17.11",
    "node-cleanup": "^2.1.2",
    "pg-promise": "^8.5.5",
    "postgraphile": "^4.3.3",
    "postgraphile-plugin-connection-filter": "^1.0.0-rc.2",
    "postgres-migrations-oasis": "^3.0.2",
    "promise-retry": "^1.1.1"
  }
}
