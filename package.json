    "express": "^4.16.4",
    "helmet": "^3.16.0",
    "lodash": "^4.17.11",
    "node-cleanup": "^2.1.2",
    "pg-promise": "^8.5.5",
    "postgraphile": "^4.3.3",
    "postgraphile-plugin-connection-filter": "^1.0.0-rc.2",
