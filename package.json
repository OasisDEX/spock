    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "10.14.2",
    "@types/pretty-hrtime": "^1.0.0",
    "@types/pretty-ms": "^5.0.1",
    "@types/promise-retry": "^1.1.1",
    "chai": "^4.2.0",
    "dotenv-flow": "^0.3.0",
    "jest": "^24.4.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "ts-essentials": "^3.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "^3.5.3",
    "typestrict": "^1.0.2"
  },
